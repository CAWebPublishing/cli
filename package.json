{
  "name": "@caweb/cli",
  "version": "1.11.1",
  "description": "CAWebPublishing Command Line Interface.",
  "exports": "./lib/env.js",
  "type": "module",
  "node": ">=20",
  "main": "./lib/env.js",
  "author": "CAWebPublishing",
  "license": "ISC",
  "bin": {
    "caweb": "bin/caweb.js"
  },
  "files": [
    "bin",
    "commands",
    "configs",
    "lib",
    "template"
  ],
  "scripts": {
    "caweb": "node bin/caweb.js",
    "doc": "node ./docs/tool/index.js",
    "test": "echo \"Error: run tests from root\" && exit 0"
  },
  "homepage": "https://github.com/CAWebPublishing/cli#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CAWebPublishing/cli.git"
  },
  "bugs": {
    "url": "https://github.com/CAWebPublishing/cli/issues"
  },
  "keywords": [
    "caweb",
    "cagov"
  ],
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "WP_VER": "6.8.1",
    "PHP_VER": "8.2",
    "DEFAULTS": {
      "FS_METHOD": "direct",
      "WP_DEBUG": true,
      "WP_DEBUG_LOG": true,
      "WP_DEBUG_DISPLAY": false,
      "WP_MEMORY_LIMIT": "256M",
      "WP_MAX_MEMORY_LIMIT": "512M",
      "WP_PERMALINK": "/%year%/%monthnum%/%postname%/",
      "WP_POST_REVISIONS": true,
      "ADMIN_COOKIE_PATH": "/",
      "COOKIE_DOMAIN": "",
      "COOKIEPATH": "",
      "SITECOOKIEPATH": "",
      "CONCATENATE_SCRIPTS": false
    }
  },
  "dependencies": {
    "@caweb/webpack": "^1.4.0-beta.3",
    "@inquirer/prompts": "^7.5.1",
    "@wordpress/create-block": "^4.66.0",
    "@wordpress/env": "^10.23.0",
    "axios": "^1.9.0",
    "axios-retry": "^4.5.0",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "cross-spawn": "^7.0.6",
    "docker-compose": "^1.2.0",
    "fs-extra": "^11.3.0",
    "html-to-json-parser": "^2.0.1",
    "inquirer-select-pro": "^1.0.0-alpha.9",
    "jsdom": "^26.1.0",
    "node-html-parser": "^7.0.1",
    "ora": "^8.2.0",
    "resolve-bin": "^1.0.1",
    "rimraf": "^6.0.1",
    "terminal-link": "^4.0.0"
  },
  "devDependencies": {
    "@caweb/a11y-webpack-plugin": "^1.0.9",
    "@caweb/css-audit-webpack-plugin": "^1.0.12",
    "@caweb/jshint-webpack-plugin": "^2.0.1"
  }
}
