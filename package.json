{
  "name": "@caweb/cli",
  "version": "1.0.0",
  "description": "CAWebPublishing Command Line Interface.",
  "main": "lib/env.js",
  "files": [
    "bin",
    "lib"
  ],
  "bin": {
    "caweb": "bin/caweb"
  },
  "scripts": {
    "caweb": "caweb",
    "launch": "caweb start",
    "pretest": "npm uninstall @caweb/cli",
    "test": "npm pack && npm i caweb-cli-%npm_package_version%.tgz",
    "posttest": "del caweb-cli-%npm_package_version%.tgz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CAWebPublishing/caweb-cli.git"
  },
  "keywords": [
    "caweb",
    "cagov"
  ],
  "author": "CAWebPublishing",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CAWebPublishing/caweb-cli/issues"
  },
  "homepage": "https://github.com/CAWebPublishing/caweb-cli#readme",
  "dependencies": {
    "@wordpress/env": "^8.11.0",
    "chalk": "^4.0.0",
    "fs-extra": "^11.1.1"
  },
  "config": {
    "WP_VER": "6.3.1",
    "PHP_VER": "8.1",
    "DEFAULTS": {
      "WP_DEFAULT_THEME": "CAWeb",
      "FS_METHOD": "direct",
      "WP_DEBUG": true,
      "WP_DEBUG_LOG": true,
      "WP_DEBUG_DISPLAY": false,
      "ADMIN_COOKIE_PATH": "/",
      "COOKIE_DOMAIN": "",
      "COOKIEPATH": "",
      "SITECOOKIEPATH": ""
    }
  },
  "directories": {
    "doc": "docs",
    "lib": "lib"
  },
  "devDependencies": {}
}
