{
  "name": "@caweb/cli",
  "version": "1.15.15",
  "description": "CAWebPublishing Command Line Interface.",
  "exports": "./lib/env.js",
  "type": "module",
  "node": ">=20",
  "main": "./lib/env.js",
  "author": "CAWebPublishing",
  "license": "ISC",
  "bin": {
    "caweb": "bin/caweb.js"
  },
  "files": [
    "bin",
    "commands",
    "configs",
    "lib",
    "template"
  ],
  "scripts": {
    "caweb": "node bin/caweb.js",
    "update:webpack:flags": "node ./scripts/webpack-flags.js",
    "doc": "node ./scripts/documentation.js",
    "test": "echo \"Error: run tests from root\" && exit 0"
  },
  "homepage": "https://github.com/CAWebPublishing/cli#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CAWebPublishing/cli.git"
  },
  "bugs": {
    "url": "https://github.com/CAWebPublishing/cli/issues"
  },
  "keywords": [
    "caweb",
    "cagov"
  ],
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "WP_VER": "6.9",
    "PHP_VER": "8.4",
    "CAWEB_VER": "1.13.3",
    "DIVI_VER": "4.27.4",
    "QUERY_MONITOR": "3.20.0",
    "DEFAULTS": {
      "FS_METHOD": "direct",
      "WP_DEBUG": true,
      "WP_DEBUG_LOG": true,
      "WP_DEBUG_DISPLAY": false,
      "WP_MEMORY_LIMIT": "256M",
      "WP_MAX_MEMORY_LIMIT": "512M",
      "WP_PERMALINK": "/%year%/%monthnum%/%postname%/",
      "WP_POST_REVISIONS": true,
      "WP_SITEURL": "http://localhost",
      "WP_HOME": "http://localhost",
      "ADMIN_COOKIE_PATH": "/",
      "COOKIE_DOMAIN": "",
      "COOKIEPATH": "",
      "SITECOOKIEPATH": "",
      "CONCATENATE_SCRIPTS": false
    }
  },
  "dependencies": {
    "@caweb/a11y-webpack-plugin": "^2.1.0",
    "@caweb/css-audit-webpack-plugin": "^2.1.0",
    "@caweb/html-webpack-plugin": "^2.1.2",
    "@caweb/jshint-webpack-plugin": "^2.1.0",
    "@caweb/webpack": "^1.6.4",
    "@inquirer/prompts": "^8.2.0",
    "@wordpress/create-block": "^4.82.0",
    "@wordpress/env": "^10.39.0",
    "axios": "^1.13.5",
    "axios-retry": "^4.5.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "cross-spawn": "^7.0.6",
    "crypto": "^1.0.1",
    "deepmerge": "^4.3.1",
    "docker-compose": "^1.3.1",
    "fs-extra": "^11.3.3",
    "html-to-json-parser": "^2.0.1",
    "inquirer-select-pro": "^1.0.0-alpha.9",
    "jsdom": "^28.0.0",
    "node-html-parser": "^7.0.2",
    "ora": "^9.3.0",
    "resolve-bin": "^1.0.1",
    "rimraf": "^6.1.2",
    "terminal-link": "^5.0.0"
  }
}
