{
  "name": "@caweb/cli",
  "version": "1.3.1",
  "description": "CAWebPublishing Command Line Interface.",
  "exports": "./lib/env.js",
  "type": "module",
  "node": ">=20",
  "author": "CAWebPublishing",
  "license": "ISC",
  "bin": {
    "caweb": "bin/caweb"
  },
  "files": [
    "assets",
    "bin",
    "commands",
    "configs",
    "gen",
    "lib",
    "template"
  ],
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 0"
  },
  "homepage": "https://github.com/CAWebPublishing/caweb-cli#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CAWebPublishing/caweb-cli.git"
  },
  "bugs": {
    "url": "https://github.com/CAWebPublishing/caweb-cli/issues"
  },
  "keywords": [
    "caweb",
    "cagov"
  ],
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "WP_VER": "6.4.3",
    "PHP_VER": "8.1",
    "CREATE_BLOCK_VER": "4.32.0",
    "DEFAULTS": {
      "FS_METHOD": "direct",
      "WP_DEBUG": true,
      "WP_DEBUG_LOG": true,
      "WP_DEBUG_DISPLAY": false,
      "WP_MEMORY_LIMIT": "256M",
      "WP_MAX_MEMORY_LIMIT": "512M",
      "WP_PERMALINK": "/%postname%/",
      "ADMIN_COOKIE_PATH": "/",
      "COOKIE_DOMAIN": "",
      "COOKIEPATH": "",
      "SITECOOKIEPATH": "",
      "CONCATENATE_SCRIPTS": false
    }
  },
  "dependencies": {
    "@wordpress/env": "^9.4.0",
    "@wordpress/scripts": "^27.3.0",
    "accessibility-checker": "^3.1.67",
    "autoprefixer": "^10.4.17",
    "axios": "^1.6.7",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "cross-spawn": "^7.0.3",
    "css-loader": "^6.10.0",
    "docker-compose": "^0.24.6",
    "handlebars-loader": "^1.7.3",
    "html-to-json-parser": "^2.0.1",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.8.0",
    "ora": "^8.0.1",
    "postcss-loader": "^8.1.0",
    "resolve-bin": "^1.0.1",
    "sass-loader": "^14.1.1",
    "terminal-link": "^3.0.0",
    "url": "^0.11.3",
    "webpack": "^5.90.3"
  }
}
