{
  "name": "@caweb/cli",
  "version": "1.2.1",
  "description": "CAWebPublishing Command Line Interface.",
  "exports": "./lib/env.js",
  "type": "module",
  "node": ">=16",
  "files": [
    "bin",
    "lib"
  ],
  "directories": {
    "doc": "docs",
    "lib": "lib"
  },
  "bin": {
    "caweb": "bin/caweb"
  },
  "scripts": {
    "caweb": "caweb",
    "wp-env": "wp-env",
    "local": "npm pack && npm i caweb-cli-%npm_package_version%.tgz",
    "test": "echo \"Error: run tests from root\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CAWebPublishing/caweb-cli.git"
  },
  "keywords": [
    "caweb",
    "cagov"
  ],
  "author": "CAWebPublishing",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CAWebPublishing/caweb-cli/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/CAWebPublishing/caweb-cli#readme",
  "dependencies": {
    "@wordpress/create-block": "^4.32.0",
    "@wordpress/env": "^9.0.0",
    "chalk": "^4.0.0",
    "commander": "^11.1.0",
    "fs-extra": "^11.1.1",
    "walker": "^1.0.8"
  },
  "config": {
    "WP_VER": "6.4.2",
    "PHP_VER": "8.1",
    "CREATE_BLOCK_VER": "4.32.0",
    "DEFAULTS": {
      "FS_METHOD": "direct",
      "WP_DEBUG": true,
      "WP_DEBUG_LOG": true,
      "WP_DEBUG_DISPLAY": false,
      "WP_MEMORY_LIMIT": "256M",
      "WP_MAX_MEMORY_LIMIT": "512M",
      "WP_PERMALINK": "/%postname%/",
      "ADMIN_COOKIE_PATH": "/",
      "COOKIE_DOMAIN": "",
      "COOKIEPATH": "",
      "SITECOOKIEPATH": "",
      "CONCATENATE_SCRIPTS": false
    }
  }
}
