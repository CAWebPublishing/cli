{
  "name": "@caweb/cli",
  "version": "1.4.1",
  "description": "CAWebPublishing Command Line Interface.",
  "exports": "./lib/env.js",
  "type": "module",
  "node": ">=20",
  "main": "./lib/env.js",
  "author": "CAWebPublishing",
  "license": "ISC",
  "bin": {
    "caweb": "bin/caweb.js"
  },
  "files": [
    "assets",
    "bin",
    "commands",
    "configs",
    "docs",
    "gen",
    "lib",
    "template"
  ],
  "scripts": {
    "doc": "node ./docs/tool/index.js",
    "test": "echo \"Error: run tests from root\" && exit 0"
  },
  "homepage": "https://github.com/CAWebPublishing/cli#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CAWebPublishing/cli.git"
  },
  "bugs": {
    "url": "https://github.com/CAWebPublishing/cli/issues"
  },
  "keywords": [
    "caweb",
    "cagov"
  ],
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "WP_VER": "6.5.4",
    "PHP_VER": "8.1",
    "DEFAULTS": {
      "FS_METHOD": "direct",
      "WP_DEBUG": true,
      "WP_DEBUG_LOG": true,
      "WP_DEBUG_DISPLAY": false,
      "WP_MEMORY_LIMIT": "256M",
      "WP_MAX_MEMORY_LIMIT": "512M",
      "WP_PERMALINK": "/%year%/%monthnum%/%postname%/",
      "ADMIN_COOKIE_PATH": "/",
      "COOKIE_DOMAIN": "",
      "COOKIEPATH": "",
      "SITECOOKIEPATH": "",
      "CONCATENATE_SCRIPTS": false
    }
  },
  "dependencies": {
    "@wordpress/create-block": "^4.44.0",
    "@wordpress/env": "^10.1.0",
    "@wordpress/scripts": "^28.1.0",
    "accessibility-checker": "^3.1.73",
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.2",
    "axios-retry": "^4.4.0",
    "chalk": "^5.3.0",
    "check-valid-url": "^0.1.0",
    "commander": "^12.1.0",
    "cross-spawn": "^7.0.3",
    "css-loader": "^7.1.2",
    "docker-compose": "^0.24.8",
    "fs-extra": "^11.2.0",
    "got": "^14.4.1",
    "handlebars-loader": "^1.7.3",
    "html-to-json-parser": "^2.0.1",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.0",
    "ora": "^8.0.1",
    "postcss-loader": "^8.1.1",
    "resolve-bin": "^1.0.1",
    "sass-loader": "^14.2.1",
    "terminal-link": "^3.0.0",
    "url": "^0.11.3",
    "webpack": "^5.92.1"
  }
}
