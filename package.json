{
  "name": "@caweb/cli",
  "version": "1.5.1",
  "description": "CAWebPublishing Command Line Interface.",
  "exports": "./lib/env.js",
  "type": "module",
  "node": ">=20",
  "main": "./lib/env.js",
  "author": "CAWebPublishing",
  "license": "ISC",
  "bin": {
    "caweb": "bin/caweb.js"
  },
  "files": [
    "bin",
    "commands",
    "configs",
    "lib",
    "template"
  ],
  "scripts": {
    "doc": "node ./docs/tool/index.js",
    "test": "echo \"Error: run tests from root\" && exit 0"
  },
  "homepage": "https://github.com/CAWebPublishing/cli#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CAWebPublishing/cli.git"
  },
  "bugs": {
    "url": "https://github.com/CAWebPublishing/cli/issues"
  },
  "keywords": [
    "caweb",
    "cagov"
  ],
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "WP_VER": "6.5.5",
    "PHP_VER": "8.1",
    "DEFAULTS": {
      "FS_METHOD": "direct",
      "WP_DEBUG": true,
      "WP_DEBUG_LOG": true,
      "WP_DEBUG_DISPLAY": false,
      "WP_MEMORY_LIMIT": "256M",
      "WP_MAX_MEMORY_LIMIT": "512M",
      "WP_PERMALINK": "/%year%/%monthnum%/%postname%/",
      "ADMIN_COOKIE_PATH": "/",
      "COOKIE_DOMAIN": "",
      "COOKIEPATH": "",
      "SITECOOKIEPATH": "",
      "CONCATENATE_SCRIPTS": false
    }
  },
  "dependencies": {
    "@caweb/webpack": "^1.0.1",
    "@wordpress/create-block": "^4.46.0",
    "@wordpress/env": "^10.3.0",
    "axios": "^1.7.2",
    "axios-retry": "^4.4.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "cross-spawn": "^7.0.3",
    "docker-compose": "^0.24.8",
    "ora": "^8.0.1",
    "resolve-bin": "^1.0.1",
    "terminal-link": "^3.0.0"
  }
}
